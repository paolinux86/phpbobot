<sardylan_db>
	<function>
		<name>startpaste</name>
		<privileged>0</privileged>
		<regex><![CDATA[/^paste (.+?) (.+)$/i]]></regex>
		<descr_name>paste {LANG} {DESCR}</descr_name>
		<descr>I'll prepare for memorize code to paste to http://rafb.net/paste/.</descr>
		<tipo>normal</tipo>
	</function>
	<function>
		<name>pasted</name>
		<privileged>0</privileged>
		<regex><![CDATA[/^pasted$/i]]></regex>
		<descr_name>pasted</descr_name>
		<descr>I'll paste the code you gave to me to http://rafb.net/paste/.</descr>
		<tipo>normal</tipo>
	</function>
	<function>
		<name>pastetypes</name>
		<privileged>0</privileged>
		<regex><![CDATA[/^pastetypes$/i]]></regex>
		<descr_name>pastetypes</descr_name>
		<descr>I'll show you what languages are supported by http://rafb.net/paste/.</descr>
		<tipo>normal</tipo>
	</function>
	<function>
		<name>paste</name>
		<tipo>always</tipo>
	</function>
</sardylan_db>