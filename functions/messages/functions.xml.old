<sardylan_db>
	<function>
		<name>getmsgs</name>
		<privileged>0</privileged>
		<regex><![CDATA[/^messages$/i]]></regex>
		<descr_name>messages</descr_name>
		<descr>You will get your unread messages</descr>
		<tipo>normal</tipo>
	</function>
	<function>
		<name>getmsgs</name>
		<privileged>0</privileged>
		<regex><![CDATA[/^allmessages$/i]]></regex>
		<descr_name>allmessages</descr_name>
		<descr>You will get all your messages</descr>
		<tipo>normal</tipo>
	</function>
	<function>
		<name>getmsgs</name>
		<privileged>0</privileged>
		<regex><![CDATA[/^messages from \b(.+)\b$/i]]></regex>
		<descr_name>messages from {USER}</descr_name>
		<descr>You will get your unread messages from {USER}</descr>
		<tipo>normal</tipo>
	</function>
	<function>
		<name>getmsgs</name>
		<privileged>0</privileged>
		<regex><![CDATA[/^allmessages from \b(.+)\b$/i]]></regex>
		<descr_name>allmessages from {USER}</descr_name>
		<descr>You will get all your messages from {USER}</descr>
		<tipo>normal</tipo>
	</function>
	<function>
		<name>getmsgs</name>
		<privileged>0</privileged>
		<regex><![CDATA[/^read ([0-9]+)$/i]]></regex>
		<descr_name>read {NUMBER}</descr_name>
		<descr>You will read your unread message number {NUMBER}</descr>
		<tipo>normal</tipo>
	</function>
	<function>
		<name>getmsgs</name>
		<privileged>0</privileged>
		<regex><![CDATA[/^readall ([0-9]+)$/i]]></regex>
		<descr_name>readall {NUMBER}</descr_name>
		<descr>You will read your message number {NUMBER}</descr>
		<tipo>normal</tipo>
	</function>
	<function>
		<name>msgsend</name>
		<privileged>0</privileged>
		<regex><![CDATA[/^message (.+?) (.+)$/i]]></regex>
		<descr_name>message {USER} {MESSAGE}</descr_name>
		<descr>You will send your message {MESSAGE} to {USER}</descr>
		<tipo>normal</tipo>
	</function>
	<function>
		<name>removemsg</name>
		<privileged>0</privileged>
		<regex><![CDATA[/^removemsg ([0-9]+)$/i]]></regex>
		<descr_name>removemsg {NUMBER}</descr_name>
		<descr>You will delete your received message number {NUMBER}</descr>
		<tipo>normal</tipo>
	</function>
	<function>
		<name>getmsgs</name>
		<tipo>join</tipo>
	</function>
</sardylan_db>